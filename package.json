{
  "name": "vot",
  "module": "src/index.ts",
  "type": "module",
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@eslint/eslintrc": "latest",
    "@eslint/js": "latest",
    "@types/bun": "latest",
    "@types/ioredis": "latest",
    "@types/node-schedule": "latest",
    "@types/numeral": "latest",
    "@types/turndown": "latest",
    "@types/user-agents": "latest",
    "pino-pretty": "latest",
    "prettier": "latest"
  },
  "scripts": {
    "fmt": "biome format --write src",
    "lint": "biome lint --write src",
    "check": "biome check --write src",
    "checkErrors": "tsc --noEmit",
    "bundle": "bun build ./src/index.ts --target bun --outfile vot.js",
    "dev": "bun --bun src/index.ts | pino-pretty",
    "start": "bunx prisma db push --accept-data-loss && NODE_ENV=production bun --bun src/index.ts | pino-pretty",
    "docker:dev": "docker-compose --file compose.dev.yml up -d"
  },
  "peerDependencies": {
    "typescript": "latest"
  },
  "dependencies": {
    "@discordjs/voice": "latest",
    "@elysiajs/cors": "latest",
    "@elysiajs/html": "latest",
    "@elysiajs/swagger": "latest",
    "@napi-rs/canvas": "latest",
    "@prisma/client": "latest",
    "axios": "latest",
    "axios-cookiejar-support": "latest",
    "cheerio": "latest",
    "discord.js": "latest",
    "elysia": "latest",
    "fuse.js": "latest",
    "genius-lyrics": "latest",
    "ioredis": "latest",
    "kazagumo": "latest",
    "kazagumo-spotify": "latest",
    "mathjs": "latest",
    "nanoid": "latest",
    "node-schedule": "latest",
    "numeral": "latest",
    "pino": "latest",
    "prisma": "latest",
    "puppeteer": "latest",
    "puppeteer-extra": "latest",
    "puppeteer-extra-plugin-adblocker": "latest",
    "puppeteer-extra-plugin-anonymize-ua": "latest",
    "puppeteer-extra-plugin-stealth": "latest",
    "query-string": "latest",
    "sharp": "latest",
    "shoukaku": "latest",
    "tough-cookie": "latest",
    "turndown": "latest",
    "user-agents": "latest",
    "bufferutil": "latest",
    "zlib-sync": "latest"
  },
  "trustedDependencies": [
    "@biomejs/biome",
    "@prisma/client",
    "@prisma/engines",
    "bufferutil",
    "cpu-features",
    "prisma",
    "protobufjs",
